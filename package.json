{
  "name": "modash",
  "version": "0.8.1",
  "description": "Modern MongoDB-inspired aggregation library for JavaScript",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/TomNeyland/modash.js"
  },
  "main": "src/modash/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/modash/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "author": "Tom Neyland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TomNeyland/modash.js/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "mocha --recursive tests/",
    "test:watch": "mocha --recursive tests/ --watch",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "build": "node scripts/build.js",
    "docs": "jsdoc -c jsdoc.conf.json",
    "type-check": "tsc --noEmit --allowJs --checkJs src/modash/*.js"
  },
  "homepage": "https://github.com/TomNeyland/modash.js",
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "chai": "^5.1.2",
    "eslint": "^9.15.0",
    "jsdoc": "^4.0.4",
    "mocha": "^10.8.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
