{
  "name": "aggo",
  "version": "1.0.0",
  "description": "Modern MongoDB-inspired aggregation library for TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aggo": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aggo",
    "mongodb",
    "aggregation",
    "pipeline",
    "typescript",
    "streaming"
  ],
  "author": "Tom Neyland",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "peerDependencies": {},
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
