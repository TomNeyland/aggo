{
  "name": "aggo-rxjs",
  "version": "1.0.0",
  "description": "RxJS integration for aggo.js - Observable-based aggregation pipelines",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aggo",
    "rxjs",
    "observable",
    "aggregation",
    "streaming",
    "reactive"
  ],
  "author": "Tom Neyland",
  "license": "MIT",
  "peerDependencies": {
    "rxjs": "^7.0.0",
    "aggo": ">=1.0.0"
  },
  "devDependencies": {
    "rxjs": "^7.8.1",
    "aggo": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/",
    "README.md"
  ]
}
