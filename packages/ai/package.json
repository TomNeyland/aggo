{
  "name": "aggo-ai",
  "version": "0.1.2",
  "description": "AI plugin for aggo.js - Natural language to MongoDB pipeline conversion using OpenAI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aggo-ai": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "mocha --import=tsx/esm tests/*.test.js",
    "test:watch": "mocha --import=tsx/esm tests/*.test.js --watch"
  },
  "keywords": [
    "aggo",
    "ai",
    "openai",
    "natural-language",
    "mongodb",
    "aggregation",
    "pipeline",
    "schema-inference"
  ],
  "author": "Tom Neyland",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.1",
    "openai": "^4.67.3",
    "zod": "^3.23.8",
    "ink": "^5.0.1",
    "ink-table": "^3.0.0",
    "ink-select-input": "^6.0.0",
    "ink-text-input": "^6.0.0",
    "asciichart": "^1.5.25",
    "chalk": "^5.3.0",
    "cli-truncate": "^4.0.0",
    "slice-ansi": "^7.1.0",
    "jsonpath-plus": "^10.2.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "chai": "^6.0.1",
    "mocha": "^11.7.2",
    "tsx": "^4.19.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "@types/react": "^18.3.11"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/",
    "README.md"
  ]
}
