{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "aggo": ["./packages/aggo/src/index.ts"],
      "aggo/*": ["./packages/aggo/src/*"]
    }
  },
  "include": ["src", "packages/aggo/src"],
  "exclude": ["dist", "node_modules"]
}
